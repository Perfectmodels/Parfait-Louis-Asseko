{
  "name": "perfect-models-management",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "node scripts/dev-cleanup.js && vite",
    "dev:clear-sw": "node scripts/clear-sw.js && vite",
    "test:setup": "node scripts/test-setup.js",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:clean": "vite preview --host",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-pwa-icons": "node scripts/generate-pwa-icons.js",
    "clear-cache": "node scripts/clear-cache.js",
    "disable-sw": "node scripts/disable-sw.js",
    "enable-sw": "node scripts/enable-sw.js",
    "cleanup": "node scripts/cleanup-project.js",
    "fix-vite": "node scripts/fix-vite-cache.js",
    "emergency-fix": "node scripts/emergency-fix.js",
    "fix-images": "node scripts/fix-image-urls.js",
    "clean": "node scripts/clean-all.js",
    "clean:quick": "powershell -Command \"Remove-Item -Recurse -Force dist -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force node_modules\\.vite -ErrorAction SilentlyContinue\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@heroicons/react": "^2.1.3",
    "@statsig/js-client": "^3.25.5",
    "@statsig/session-replay": "^3.25.5",
    "@statsig/web-analytics": "^3.25.5",
    "firebase": "^12.3.0",
    "framer-motion": "^12.23.12",
    "fs": "^0.0.1-security",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.7"
  }
}